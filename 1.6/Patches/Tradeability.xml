<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>

			
			<!-- Add tradeability to all arcane base weapons -->
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[starts-with(@Name, 'RF_BaseWeapon_Arcane')]</xpath>
				<value>
					<tradeability>All</tradeability>
				</value>
			</li>


			
		</operations>
	</Operation>
</Patch>